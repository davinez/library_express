{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "operator-linebreak": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0
  }
}
